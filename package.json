{
  "name": "niximages",
  "version": "0.0.2",
  "license": "MIT",
  "private": true,
  "description": "Docker [devcontainer](https://code.visualstudio.com/docs/remote/containers) images built to be used with the Nix package manager with declarative development environments defined using nix flakes.",
  "homepage": "https://github.com/devtemplates/niximages",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devtemplates/niximages.git"
  },
