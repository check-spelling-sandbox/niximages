    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "test": "vitest"
  },
  "dependencies": {
    "@octokit/rest": "21.0.2",
    "apt-parser": "1.5.1",
    "axios": "1.6.5",
    "semver": "7.5.4",
    "tsx": "4.19.2"
  },
  "devDependencies": {
    "@devtemplates/eslint-preset-typescript": "devtemplates/eslint-preset-typescript#main",
    "@types/dockerode": "^3.3.31",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.53.0",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "9.1.5",
    "lint-staged": "15.2.9",
    "msw": "^2.4.6",
    "prettier": "3.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "5.0.0",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  }
